@model IEnumerable<Models.ReviewedMovie>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        @foreach (var movie in Model)
    {
        <div class="col-md-4 movie">
            <p>@movie.UserID</p>
            <p>@movie.MovieID</p>
            <h1>@movie.UserRating</h1>

        </div>
}
    </div>
</body>
</html>
